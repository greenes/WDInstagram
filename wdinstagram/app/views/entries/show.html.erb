<h1>Photo# <%=@entry.id%></h1>

  <img src="<%=@entry.photo_url%>" alt="photo">
  <p>created <%=@entry.created_at.to_s%></p>

  <h2><a href="/entries/<%=@entry.id%>/edit">Update this photo!</a></h2>

  <%= form_tag entry_path(@entry), method: "delete" do %>
    <%= submit_tag("delete this photo")%>
    <%end%>
